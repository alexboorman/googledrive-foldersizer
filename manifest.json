{
	"name": "Google Drive Folder Size",
	"manifest_version": 2,
	"version": "0.1",
	"icons": {
	"128": "128.png"
	},

	"content_scripts": [
		{
			"matches": [
				"http://drive.google.com/*",
				"https://drive.google.com/*"
			],
			"js": ["script.js"],
			"run_at": "document_start"
		}
	],
	"permissions": [
		"activeTab"
	]
}
